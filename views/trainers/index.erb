<h1> Trainer List: </h1>

<% @trainers.each do |trainer| %>
  <p><b><a href="/trainers/<%= trainer.id %>"></b> <%= trainer.name %></a></p>
  <p><b> Hometown:</b> <%= trainer.hometown %></p>
  <p><b> Score:</b>
   <%= trainer.points %></p>
  <hr/>
<% end %>
  <p><a href="/trainers/new"> Add Trainer </a></p>
